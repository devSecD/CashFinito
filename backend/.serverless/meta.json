    "serviceRawFile": "service: cashfinito-api\n\n# Set your team ID if you are using Bref Cloud\n#bref:\n#    team: my-team-id\n\nprovider:\n    name: aws\n    region: us-east-1\n    stage: production\n    runtime: php-83\n    environment:\n        APP_ENV: production\n        APP_DEBUG: false\n        LOG_CHANNEL: stderr\n        DB_CONNECTION: pgsql\n        DB_HOST: ${env:DB_HOST}\n        DB_PORT: ${env:DB_PORT}\n        DB_DATABASE: ${env:DB_DATABASE}\n        DB_USERNAME: ${env:DB_USERNAME}\n        DB_PASSWORD: ${env:DB_PASSWORD}\n        SESSION_DRIVER: cookie\n        CACHE_DRIVER: array\n        QUEUE_CONNECTION: sync\n        DB_SSLMODE: require\n    vpc:\n        securityGroupIds:\n            - ${env:SG_LAMBDA} # agregar al env\n        subnetIds:\n            - ${env:SUBNET_PRIVATE_1A}\n            - ${env:SUBNET_PRIVATE_1B}\n\npackage:\n    # Files and directories to exclude from deployment\n    patterns:\n        - '!node_modules/**'\n        - '!public/storage'\n        - '!resources/assets/**'\n        - '!resources/css/**'\n        - '!resources/images/**'\n        - '!resources/js/**'\n        - '!storage/**'\n        - '!tests/**'\n        - '!database/*.sqlite'\n        - '!.git/**'\n        # Exclude assets except for the manifest file\n        - '!public/build/**'\n        - 'public/build/manifest.json'\n        - 'php/**'\n\nfunctions:\n    # This function runs the Laravel website/API\n    web:\n        handler: public/index.php\n        timeout: 28 # in seconds (API Gateway has a timeout of 29 seconds)\n        layers:\n            - ${bref:layer.php-83-fpm}\n        events:\n            - httpApi: '*'\n\n    # This function lets us run artisan commands in Lambda\n    artisan:\n        handler: artisan\n        timeout: 120 # in seconds\n        layers:\n            - ${bref:layer.php-83-fpm}      # ← Capa base de PHP con extensiones\n            - ${bref:layer.console}      # ← Capa de consola\n\nplugins:\n    # We need to include the Bref plugin\n    - ./vendor/bref/bref\n    - serverless-lift\n\ncustom:\n    lift:\n        constructs:\n            storage:\n                type: storage",
